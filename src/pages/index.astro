---
import Layout from "@layouts/Layout.astro";
import VerticalStepper from "@components/vertical-stepper";

export const prerender = true;

const generated = [
    ["11/05/2023 - Current", "Changed this and that", "Very cool"],
    ["10/26/2023 - 11/04/2023", "NA"],
    ["10/18/2023 - 10/25/2023", "This", "And", "that", "Changed"],
];
---

<Layout
    title="Autumn-cl"
    desc="Autogenerated changelogs!"
    mainClasses="flex flex-col items-center justify-center w-full h-full text-lg"
>
    <div class="min-w-[50vw]">
        <VerticalStepper generated={generated} />
    </div>
</Layout>
